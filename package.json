{
  "name": "e-hotels",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-front-end": "cd front-end && npm install && npm run build",
    "heroku-postbuild": "npm run build-front-end",
    "debug-back-end": "nodemon ./bin/www",
    "debug-front-end": "cd front-end && npm run start",
    "debug": "concurrently \"npm run debug-back-end\" \"npm run debug-front-end\"",
    "start-all": "npm run build-front-end && node ./bin/www",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "pg": "^7.8.0",
    "camelcase": "^5.0.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "nodemon": "^1.18.10",
    "concurrently": "^4.1.0"
  }
}
